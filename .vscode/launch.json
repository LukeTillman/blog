  "version": "0.2.0",
  "configurations": [
    {
      "name": "Hexo Generate",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/hexo/bin/hexo",
      "args": [
        "generate"
      ],
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Hexo Serve",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/hexo/bin/hexo",
      "args": [
        "serve"
      ],
      "cwd": "${workspaceRoot}"
    }
  ]
}